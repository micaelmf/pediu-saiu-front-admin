{
    "name": "skewind-html",
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "dev": "npm run theme:preval && nodemon server.js",
        "build": "npm run theme:preval && vite build --emptyOutDir",
        "preview": "vite preview",
        "theme:preval": "node ./src/@skewind/bin/themePreval.js --input tailwind.config.js --output src",
        "icon:build": "node ./src/@skewind/bin/icon.js -p src/icons",
        "markup:validate": "node ./src/@skewind/bin/w3c.js -h http://localhost:5173",
        "search:index": "node ./src/@skewind/bin/search.js -h http://localhost:5173 -o public",
        "sync": "browser-sync start --proxy 'localhost:3002' --files '**/*' --ignore 'node_modules'"
    },
    "dependencies": {
        "@svg-icons/material-outlined": "^1.86.0",
        "@svg-icons/material-twotone": "^1.86.0",
        "@tailwindcss/forms": "^0.5.2",
        "@tailwindcss/line-clamp": "^0.4.4",
        "@tailwindcss/typography": "^0.5.2",
        "@tiptap/core": "^2.0.4",
        "@tiptap/extension-text-align": "^2.0.4",
        "@tiptap/extension-underline": "^2.0.4",
        "@tiptap/pm": "^2.0.4",
        "@tiptap/starter-kit": "^2.0.4",
        "apexcharts": "^3.35.0",
        "axios": "^1.6.0",
        "browser-sync": "^2.29.3",
        "cookie-parser": "^1.4.6",
        "crel": "^4.2.1",
        "dotenv": "^16.3.1",
        "dripicons": "^2.0.0",
        "dropzone": "^6.0.0-beta.2",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "feather-icons": "^4.29.0",
        "flag-icons": "^6.6.0",
        "flat-color-icons": "^1.1.0",
        "flatpickr": "^4.6.13",
        "fuse.js": "^6.6.2",
        "heroicons": "^2.0.18",
        "highlight.js": "^11.5.1",
        "html-entities": "^2.3.3",
        "ionicons": "^7.1.2",
        "js-beautify": "^1.14.4",
        "lodash": "^4.17.21",
        "rollup-plugin-ejs": "^4.0.0",
        "simplebar": "^6.2.5",
        "tailwindcss": "^3.1.3",
        "tippy.js": "^6.3.7",
        "toastify-js": "^1.12.0",
        "tom-select": "^2.0.1",
        "vite-express": "^0.10.0",
        "vite-plugin-ejs": "^1.6.4"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.14",
        "cheerio": "^1.0.0-rc.10",
        "crawler": "^1.3.0",
        "esm": "^3.2.25",
        "html-minifier": "^4.0.0",
        "minimist": "^1.2.6",
        "nodemon": "^3.0.1",
        "postcss": "^8.1.14",
        "prettier": "^3.0.0",
        "prettier-plugin-tailwindcss": "^0.4.1",
        "vite": "^4.4.4",
        "w3c-html-validator": "^1.1.0"
    }
}
